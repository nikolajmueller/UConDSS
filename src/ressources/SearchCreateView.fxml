            <TextField fx:id="field_cpr" layoutX="86.0" layoutY="118.0" />
            <Label layoutX="24.0" layoutY="169.0" text="Name:" />
            <Label layoutX="20.0" layoutY="258.0" text="Gender:" />
            <TextField fx:id="field_name" layoutX="86.0" layoutY="164.0" />
            <Button fx:id="button_logout" layoutX="223.0" layoutY="14.0" mnemonicParsing="false" text="Log out" />
            <Label fx:id="label_toSavePatientFillInBlancFields" layoutX="30.0" layoutY="317.0" prefHeight="17.0" prefWidth="239.0" />
         </children>
      </AnchorPane>
   </children>

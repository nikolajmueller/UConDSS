<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="STproject.Controllers.SearchCreateViewController">
    <stylesheets>
        <URL value="@SearchCreateView.css" />
    </stylesheets>
   <children>
      <AnchorPane prefHeight="398.0" prefWidth="305.0">

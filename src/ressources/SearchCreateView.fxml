                  <Font name="System Bold" size="18.0" />
               </font></Label>
            <TextField fx:id="keywordTextField" layoutX="72.0" layoutY="101.0" promptText="Enter patient CPR" />
            <TableView fx:id="tableView_CPR" layoutX="38.0" layoutY="139.0" prefHeight="200.0" prefWidth="230.0">
              <columns>
                <TableColumn fx:id="col_CPR" prefWidth="227.0" text="CPR" />
              </columns>
            </TableView>
            <Button layoutX="121.0" layoutY="359.0" mnemonicParsing="false" onAction="#btnToDashboard" text="KNAP" />
         </children>
      </AnchorPane>
    <AnchorPane layoutX="309.0" minHeight="0.0" minWidth="0.0" prefHeight="398.0" prefWidth="292.0">
         <children>
            <Button fx:id="btnSavePatient" layoutX="192.0" layoutY="357.0" mnemonicParsing="false" onAction="#btnSavePatientFunc" text="Save patient" />
